{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "script"
  },
  "rules": {
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  }
}
